{
  "ignore": [
    ".git",
    "node_modules/**/node_modules",
    "definitions/node_modules/**/node_modules"
  ],
  "verbose": false,
  "execMap": {
    "js": "node --harmony"
  },
  "watch": [
    "app/",
    "config/",
    "definitions/",
    "shared/",
    "src/"
  ],
  "env": {
    "NODE_ENV": "development"
  },
  "ext": "ts"
}
